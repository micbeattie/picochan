PROJECT_NAME            = "Picochan"
PROJECT_BRIEF           = "Picochan documentation"
PROJECT_NUMBER          = 0.1

STRIP_FROM_PATH        = @PICOCHAN_SOURCE_DIR@

INPUT                  =  @PICOCHAN_SOURCE_DIR@

FILE_PATTERNS          = *.h \
                         *.cpp \
                         *.c \
                         *.S \
                         *.s \
                         *.md

USE_MDFILE_AS_MAINPAGE = @PROJECT_SOURCE_DIR@/mainpage.md

OPTIMIZE_OUTPUT_FOR_C = YES
GENERATE_TREEVIEW = YES
GENERATE_LATEX = YES
USE_PDFLATEX = YES
PDF_HYPERLINKS = YES
GENERATE_XML = YES
GROUP_GRAPHS = NO

OUTPUT_DIRECTORY       = @DOXY_OUTPUT_DIR@
RECURSIVE              = YES

EXTRACT_STATIC         = YES
EXTRACT_ALL            = NO
ALWAYS_DETAILED_SEC    = NO

ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
PREDEFINED             = __not_in_flash_func(x)= \
                         __time_critical_func(x)= \
                         __not_in_flash(x)= \
                         __no_inline_not_in_flash(x)= \
                         __attribute__(x)= \
                         __aligned(x)= \
                         DOXYGEN_GENERATION=
